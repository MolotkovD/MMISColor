{
  "name": "Theme redacter",
  "description": " ",
  "version": "1.0",
  "offline_enabled": true,
  "manifest_version": 3,
  "icons": {
      "16":"icons/icon16.png",
      "48":"icons/icon48.png",
      "128":"icons/icon128.png"
  },
  "action": {
    "default_popup":"popup/popup.html"
},
"permissions": ["scripting", "activeTab", "storage", "tabs"],
"background": {
  "service_worker": "background/background.js"
},
"content_scripts": [
    {
      "js": ["SettUpRules.js"],
      "matches": ["https://mmis-web.rudn-sochi.ru/WebApp/*"]
    }
  ],
  "host_permissions": [
    "https://mmis-web.rudn-sochi.ru/WebApp/*"
  ]
}
 
